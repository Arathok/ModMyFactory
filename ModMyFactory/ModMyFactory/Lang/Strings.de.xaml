    <sys:String x:Key="AuthorLabel">von Mathis Rech</sys:String>
    <sys:String x:Key="FlagsLabel">Flaggenbilder:</sys:String>
    <sys:String x:Key="FontLabel">Factorio-Schriftart:</sys:String>
    <sys:String x:Key="DialogsLabel">Dialoge:</sys:String>
    <sys:String x:Key="JsonLabel">JSON Framework:</sys:String>

